{
  "version": 2,
  "name": "nexus-scanner",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/build",
  "installCommand": "cd web && npm install"
}